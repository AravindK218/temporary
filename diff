Terraform

provider "aws" {region= "us_east_1"}
resource "aws_instance" "web" {
 ami= "${ami_id}"
 instance_type="t2.micro"
 key-name="terraform"
 security_groups_ids=
 tags {
   Name = "Hello-Terraform"
   }
 }
